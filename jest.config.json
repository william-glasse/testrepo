{
  "testEnvironment": "node",
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.js", "src/*.js"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "clearMocks": true,
  "resetMocks": true,
  "resetModules": true
}
